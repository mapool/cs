.{$ui-prefix}text {
    &.primary {
        color: $brand-primary;
    }
    &.secondary {
        color: $brand-secondary;
    }
    &.success {
        color: $brand-success;
    }
    &.error {
        color: $brand-error;
    }
    &.info {
        color: $brand-info;
    }
    &.warning {
        color: $brand-warning;
    }
    &.danger {
        color: $brand-danger;
    }
    &.muted {
        color: $brand-muted;
    }
}

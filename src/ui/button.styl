.btn-default,
.btn-primary,
.btn-info,
.btn-success,
.btn-error {
    ui-size('btn', true, true);
}


.btn-default {
    btn-default();
}

.btn-primary {
    btn-primary();
}

.btn-info {
    btn-info();
}

.btn-success {
    btn-success();
}

.btn-error {
    btn-error();
}

.btn-link {
    btn-link();
    ui-size('btn', false, true);
}
